{
  "name": "node-boilerplate",
  "version": "0.1.0",
  "description": "node-boilerplate",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rimraf coverage .nyc_output",
    "fmt": "prettier --write ./**/*.js",
    "lint": "eslint --fix ./**/*.js",
    "report": "nyc report --reporter=html",
    "test": "nyc ava",
    "test:coverage": "run-s clean fmt lint test report",
    "precommit": "lint-staged",
    "prepush": "ava"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/70-10/node-boilerplate.git"
  },
  "keywords": [],
  "author": "70_10",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/70-10/node-boilerplate/issues"
  },
  "homepage": "https://github.com/70-10/node-boilerplate#readme",
  "dependencies": {
    "config": "^1.25.1",
    "debug": "^2.6.3"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^2.0.1",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.2.0",
    "prettier": "^1.1.0",
    "rimraf": "^2.6.1"
  },
  "ava": {
    "files": [
      "./test/**/*.test.js"
    ]
  }
}
